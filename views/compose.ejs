
<%- include("partials/header") -%>
<h1 class="text-center">Compose</h1>
<div class="container">
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="postTitle">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
  </div>
  <button class="btn btn-outline-info mt-2" type="submit" name="button">Publish</button>
</form>
</div>
<%- include("partials/footer") -%>